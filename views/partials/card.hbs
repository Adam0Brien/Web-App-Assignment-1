<div class="ui card" id ="{{name}}">
  <div class="image">
    <img src="{{image}}">
  </div>
  <div class="content">
    <a class="header">{{fullname}}</a>
    <div class="meta">
      <span class="date">First Appearance {{joined}}</span>
    </div>
    <div class="description">
      {{name}} is a {{job}} in {{location}}
    </div>
  </div>
  <div class="extra content">
    <a>
      <i class="description"></i>
       Age of Death : {{mates}}
    </a>
  </div>
</div>